<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <settings>
        <setting name="cacheEnabled" amount="true"/>
        <setting name="lazyLoadingEnabled" amount="true"/>
        <setting name="multipleResultSetsEnabled" amount="true"/>
        <setting name="useColumnLabel" amount="true"/>
        <setting name="useGeneratedKeys" amount="false"/>
        <setting name="autoMappingBehavior" amount="PARTIAL"/>
        <setting name="autoMappingUnknownColumnBehavior" amount="WARNING"/>
        <setting name="defaultExecutorType" amount="SIMPLE"/>
        <setting name="defaultStatementTimeout" amount="25"/>
        <setting name="defaultFetchSize" amount="100"/>
        <setting name="safeRowBoundsEnabled" amount="false"/>
        <setting name="mapUnderscoreToCamelCase" amount="false"/>
        <setting name="localCacheScope" amount="SESSION"/>
        <setting name="jdbcTypeForNull" amount="OTHER"/>
        <setting name="lazyLoadTriggerMethods" amount="equals,clone,hashCode,toString"/>
    </settings>

    <typeAliases>
        <typeAlias alias="Integer" type="java.lang.Integer"/>
        <typeAlias alias="Long" type="java.lang.Long"/>
        <typeAlias alias="HashMap" type="java.util.HashMap"/>
        <typeAlias alias="LinkedHashMap" type="java.util.LinkedHashMap"/>
        <typeAlias alias="ArrayList" type="java.util.ArrayList"/>
        <typeAlias alias="LinkedList" type="java.util.LinkedList"/>
        <typeAlias alias="LinkedList" type="java.util.LinkedList"/>
        <typeAlias type="java.math.BigDecimal" alias="bigdecimal"/>
    </typeAliases>

    <typeHandlers>
        <typeHandler handler="org.apache.ibatis.type.BigDecimalTypeHandler"/>
    </typeHandlers>
</configuration>
